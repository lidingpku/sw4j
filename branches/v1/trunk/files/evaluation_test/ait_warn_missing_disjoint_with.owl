<?xml version="1.0"?>
<!-- 
	the instance of foaf:Person is also instance of foaf:Group.
	no subclass/superclass relation between foaf:Person and foaf:Group in foaf ontology.
 -->

<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/">
 <foaf:Person>
      <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
      <foaf:knows>Jacky</foaf:knows>
  </foaf:Person>
</rdf:RDF>