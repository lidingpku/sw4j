<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
     <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
     <!ENTITY wine  "http://tw.rpi.edu/2008/04/wine-lite.owl#" >
     <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
   ]>
<rdf:RDF
  xmlns:wine = "&wine;"
  xmlns:rdf = "&rdf;">


<!-- 
 A wine instance having the wrong value for hasMaker.
 Therefore, we should report a warning indicating the type of the value
 of hasMaker does not match the expected type.
-->
<wine:Zinfandel rdf:about="#W2">
  <wine:hasColor rdf:resource="&wine;Red"/>
  <wine:hasFlavor rdf:resource="&wine;Moderate"/>
  <wine:hasSugar rdf:resource="&wine;Dry"/>
  <wine:hasBody rdf:resource="&wine;Full"/>
  <wine:hasMaker rdf:resource="&wine;Red"/>
</wine:Zinfandel>

</rdf:RDF>